{
  "name": "comfyui-example-react-extension",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npx tsc && npx vite build",
    "watch": "npx tsc && npx vite build --watch",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "typecheck": "npx tsc --noEmit",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "npm run typecheck && npm run lint && npm test",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.25",
    "@assistant-ui/react": "^0.12.9",
    "@assistant-ui/react-ai-sdk": "^1.3.6",
    "@assistant-ui/react-markdown": "^0.12.3",
    "@assistant-ui/styles": "^0.3.7",
    "@tailwindcss/vite": "^4.1.18",
    "ai": "^6.0.73",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^23.10.2",
    "i18next-browser-languagedetector": "^7.2.2",
    "i18next-http-backend": "^2.5.0",
    "lucide-react": "^0.563.0",
    "radix-ui": "^1.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.25.1",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@comfyorg/comfyui-frontend-types": "^1.20.2",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.27.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.2.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.1.0",
    "husky": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "typescript": "^5.4.2",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.4.1"
  }
}
